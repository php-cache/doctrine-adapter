{
    "name":              "cache/doctrine-adapter",
    "description":       "A PSR-6 Cache Implementation using Doctrine",
    "type":              "library",
    "license":           "MIT",
    "minimum-stability": "stable",
    "keywords":          [
        "cache",
        "psr-6",
        "doctrine"
    ],
    "homepage":          "https://github.com/php-cache/doctrine-adapter",
    "authors":           [
        {
            "name":     "Aaron Scherer",
            "email":    "aequasi@gmail.com",
            "homepage": "https://github.com/aequasi"
        },
        {
            "name":     "Tobias Nyholm",
            "email":    "tobias.nyholm@gmail.com",
            "homepage": "https://github.com/nyholm"
        }
    ],
    "require":           {
        "psr/cache":            "1.0.0",
        "cache/taggable-cache": "^0.1",
        "doctrine/cache":       "^1.3"
    },
    "require-dev":       {
        "phpunit/phpunit":         "^5.1|^4.0",
        "mockery/mockery":         "^0.9.4",
        "cache/integration-tests": "dev-master"
    },
    "provide":           {
        "psr/cache-implementation": "1.0.0"
    },
    "autoload":          {
        "psr-4": {
            "Cache\\Doctrine\\": "src/"
        }
    },
    "autoload-dev":      {
        "psr-4": {
            "Cache\\Doctrine\\Tests\\": "tests/"
        }
    }
}
